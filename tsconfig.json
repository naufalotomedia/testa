{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "moduleResolution": "bundler",
    "baseUrl": "./",
    "paths": {
      "@components/*": ["src/components/*"],
      "@assets/*": ["assets/*"],
      "@redux/*": ["src/redux/*"],
      "@screen/*": ["src/screens/*"],
      "@utils/*": ["src/utils/*"],
      "@wrapper/*": ["src/screens/wrapper.tsx"],
      "@controllers/*": ["src/controllers/*"],
      "@navigation/*": ["src/navigations/*"],
      "@api/*": ["src/api/*"],
    }
  }
}
